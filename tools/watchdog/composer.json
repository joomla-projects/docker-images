{
    "name": "joomla/watchdog",
    "description": "Different watchers for different repos in the Joomla!-sphere",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "repositories": [
      {
        "type": "vcs",
        "url": "https://github.com/joomla-backports/php-tuf.git",
        "no-api": true
      },
      {
        "type": "vcs",
        "url": "https://github.com/elasticemail/elasticemail-php.git"
      }
    ],
    "autoload": {
        "psr-4": {
            "Joomla\\Watchdog\\": "src/"
        }
    },
    "require": {
        "php-tuf/php-tuf": "dev-main",
        "joomla/http": "~3.0",
        "elasticemail/elasticemail-php": "*@dev"
    },
    "config": {
        "allow-plugins": {
            "cweagans/composer-patches": false
        }
    }
}
